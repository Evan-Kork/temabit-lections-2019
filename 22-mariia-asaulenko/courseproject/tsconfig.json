{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "es2019",
      "DOM"
    ],
    "allowJs": true,
    "resolveJsonModule": true,
    "experimentalDecorators":true
  },
  
  "include": [
    "./src/**/**/*"
  ]
}